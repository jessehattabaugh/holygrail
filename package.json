{
  "name": "cards",
  "version": "0.0.0",
  "description": "flashcard PWA",
  "engines": {
    "node": "7.10.0",
    "npm": "4.2.0"
  },
  "public": false,
  "scripts": {
    "bundle-client": "webpack --colors --env.target client",
    "bundle-server": "webpack --colors --env.target server",
    "check": "tsc",
    "format": "prettier --write --trailing-comma all --use-tabs --single-quote \"{,.,!(node_modules)/**/}*.js\"",
    "kill": "killall -9 node && killall -9 npm",
    "lint": "eslint \"**/*.js\" --ignore-path .gitignore --quiet",
    "precommit": "npm run format && npm test",
    "server": "node ./dist/server/server.bundle.js",
    "start-dev": "DEBUG=express:* npm run bundle-server -- --env.watch --env.runServerAfterBundle --env.hotClient --env.hotServer",
    "start": "npm run bundle-server && npm run bundle-client && npm run server",
    "test": "npm run lint && npm run check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StudyBlue/cards.git"
  },
  "contributors": [
    "Jesse Hattabaugh<jesse@studyblue.com>"
  ],
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/StudyBlue/cards/issues"
  },
  "homepage": "https://github.com/StudyBlue/cards#readme",
  "devDependencies": {
    "@types/chalk": "^0.4.31",
    "@types/express": "^4.0.35",
    "@types/minimist": "^1.2.0",
    "@types/node": "^7.0.22",
    "@types/react": "^15.0.25",
    "@types/react-dom": "^15.5.0",
    "@types/webpack": "^2.2.15",
    "@types/webpack-dev-middleware": "^1.9.1",
    "@types/webpack-env": "^1.13.0",
    "@types/webpack-hot-middleware": "^2.15.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chalk": "^1.1.3",
    "clean-webpack-plugin": "^0.1.16",
    "eslint": "^3.19.0",
    "eslint-config-prettier": "^2.1.1",
    "eslint-plugin-prettier": "^2.1.1",
    "eslint-plugin-react": "^7.0.1",
    "express": "^4.15.3",
    "husky": "^0.13.4",
    "minimist": "^1.2.0",
    "prettier": "^1.3.1",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "typescript": "^2.3.3",
    "webpack": "^2.6.1",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-node-externals": "^1.6.0",
    "webpack-shell-plugin": "^0.5.0"
  }
}
