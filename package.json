{
  "name": "cards",
  "version": "0.0.0",
  "description": "flashcard PWA",
  "engines": {
    "node": "7.10.0",
    "npm": "4.2.0"
  },
  "public": false,
  "scripts": {
    "test": "npm run lint",
    "lint": "eslint \"**/*.js\" --ignore-path .gitignore --quiet",
    "bundle-server": "webpack --colors --env.target server",
    "bundle-client": "webpack --colors --env.target client",
    "format": "prettier --write --trailing-comma all --use-tabs --single-quote \"{,.,!(node_modules)/**/}*.js\"",
    "precommit": "npm run format && npm run lint",
    "start": "if NODE_ENV == 'production' npm run start-prod else npm run start-dev",
    "start-dev": "npm run bundle-server -- --env.watch --env.runServerAfterBundle",
    "start-prod": "npm run bundle-server && npm run bundle-client && npm run server",
    "server": "node ./server/server.bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StudyBlue/cards.git"
  },
  "contributors": [
    "Jesse Hattabaugh<jesse@studyblue.com>"
  ],
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/StudyBlue/cards/issues"
  },
  "homepage": "https://github.com/StudyBlue/cards#readme",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-prettier": "^1.7.0",
    "eslint-plugin-prettier": "^2.0.1",
    "eslint-plugin-react": "^6.10.3",
    "express": "^4.15.2",
    "husky": "^0.13.3",
    "prettier": "^1.3.1",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "webpack": "^2.5.0",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-node-externals": "^1.5.4",
    "webpack-shell-plugin": "^0.5.0"
  }
}
