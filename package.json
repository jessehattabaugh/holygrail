{
  "name": "cards",
  "version": "0.0.0",
  "description": "flashcard PWA",
  "engines": {
    "node": "7.10.0",
    "npm": "4.2.0"
  },
  "public": false,
  "scripts": {
    "test": "npm run lint",
    "lint": "eslint \"**/*.js\" --ignore-path .gitignore --quiet",
    "bundle-server": "webpack --env.target server",
    "bundle-client": "webpack --env.target client",
    "format": "prettier --write --trailing-comma all --use-tabs --single-quote \"{,.,!(node_modules)/**/}*.js\"",
    "precommit": "npm run format && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StudyBlue/cards.git"
  },
  "contributors": [
    "Jesse Hattabaugh<jesse@studyblue.com>"
  ],
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/StudyBlue/cards/issues"
  },
  "homepage": "https://github.com/StudyBlue/cards#readme",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-syntax-trailing-function-commas": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-prettier": "^1.7.0",
    "eslint-plugin-prettier": "^2.0.1",
    "eslint-plugin-react": "^6.10.3",
    "husky": "^0.13.3",
    "prettier": "^1.3.1",
    "webpack": "^2.5.0",
    "webpack-node-externals": "^1.5.4"
  }
}
